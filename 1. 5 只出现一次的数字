# 我自己的代码
class Solution:
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        nums.sort()
        for i in range(0, len(nums)-1, 2):
            if(nums[i] != nums[i+1]):
                return nums[i]
        return nums[len(nums)-1]
        
# 使用字典
class Solution:
    def singleNumber(self, nums):
        dict = {}
        for i in range(len(nums)) :
            if i in dict.key() :
                dict[i] += 1
            else :
                dict[i] = 1
        
        for j in dict :
            if dict[j] == 1
                return j
      
